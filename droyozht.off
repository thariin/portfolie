h1 {
    font-family: Arial Black;
    font-size: 30px;
    color: #000;
    text-align: center;
}

h2 {
    font-family: Arial;
    font-size: 30px;
    color: #000;
    text-align: center;
}

p {
    font-family: Arial;
    font-size: 15px;
    color: #000;
    /*text-align:center;*/
}

p2 {
    font-family: Arial;
    font-size: 15px;
    color: #000;
}

html {
    width: 100%;
    height: 100%;
}

body {
    margin: auto;
    width: 1000px;
    height: 100%;
    background: -webkit-linear-gradient(white, blue); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(white, blue); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(white, blue); /* For Firefox 3.6 to 15 */
    background: linear-gradient(white, blue); /* Standard syntax */
    background-size: 100%;
    background-repeat: no-repeat;
}

#header {
    margin: auto;
    margin-top: 50px;
    height: 75px;
    width: 100%;
    padding-top: 1px;
    background-color: #666;
}

#menu {
    margin: auto;
    height: 70px;
    width: 100%;
    background-color: #666;
    margin-top: 35px;
    border-radius: 5px;
    text-align: center;
}

#menulist {
    float: left;
    width: 100%;
    padding: 0;
    margin: 0;
    list-style-type: none;
}
li {
    display: inline;
}

#content {
    margin: auto;
    height: 480px;
    width: 100%;
    background-color: #666;
    margin-top: 35px;
    border-radius: 5px;
}

#contenttext {
    margin: auto;
    height: 440px;
    width: 100%;
    padding: 20px;
    margin-bottom: 5px;
    overflow: auto;
}

.projektgrid {
    margin: auto;
    width: auto;
    font-family: 'Electrolize', sans-serif;
    font-size: 1em;
    /* border-bottom: 1px solid #000; */
}

.cell {
    width: 100px;
    height: 100px;
    /*margin: auto;*/
    margin-top: 15px;
    margin-left: 15px;
    border: solid 1px #000000;
    float: left;
    background-image: url(billeder/COLOURBOX5897716.jpg);
    background-size: 100%;
}

